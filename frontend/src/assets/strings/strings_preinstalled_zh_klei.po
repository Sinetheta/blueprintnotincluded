msgctxt "STRINGS.BUILDINGS.PREFABS.FLOWERVASE.FACADES.RETRO_ELEGANT.NAME"
msgid "<link=\"FLOWERVASE\">Elegant Retro Flower Pot</link>"
msgstr "<link=\"FLOWERVASE\">优雅复古花盆</link>"

#. STRINGS.BUILDINGS.PREFABS.FLOWERVASE.FACADES.RETRO_SUNNY.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.FLOWERVASE.FACADES.RETRO_SUNNY.DESC"
